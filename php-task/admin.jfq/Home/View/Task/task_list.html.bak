        //任务上架下架操作
        function invalid(type,id)
        { 
            if(type == 1){ 
                var url = '/admin/task/invalid.html';
             }else{ 
                var url = '/admin/task/delete.html';
             }

            $.ajax({ 
                url:url,
                type:"post",
                data:{ 'id':id },
                dataType:'json',
                success:function(ret){ 
                    if(ret.code){ 
                        alert(ret.msg);
                        location.reload();
                     }else{ 
                        alert(ret.msg);
                     }
                 }
             })
         }
